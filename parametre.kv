<Parametre>:
    FloatLayout:
        MDLabel:
            id: our_label
            text:"ici Parametre"
            halign:"center"

        MDRoundFlatButton:
            text:"back to main"
            pos_hint:{"center_x":.5, "center_y":.2}
            on_release:
                app.change_fenetre(nom_fenetre="accueil")
        
        MDRoundFlatButton:
            text:"Liste"
            pos_hint:{"center_x":.5, "center_y":.4}
            on_release:
                for i in range(5): our_label.text= our_label.text+ "\nLooped!"
              